{
  "expo": {
    "scheme": "georent",
    "owner": "georent",
    "name": "GeoRent",
    "slug": "GeoRent-Mobile",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "primaryColor": "#ffffff",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "react-native-auth0",
        {
          "domain": "georent-mobile.us.auth0.com"
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash.png",
      "imageResizeMode": "contain",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "assets/**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false,
        "googleMapsApiKey": "AIzaSyB8ZY1bvvsQpYvq9JFLcBUEY3QoyEwJchg"
      },
      "bundleIdentifier": "com.prod.georentmobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.prod.georentmobile",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyB8ZY1bvvsQpYvq9JFLcBUEY3QoyEwJchg"
        }
      }
    },
    "extra": {
      "eas": {
        "projectId": "8fc22f1b-9fb9-498f-b791-6b5c5396e5cf"
      },
      "googleApiKey": "@env:GOOGLE_API_KEY"
    }
  }
}
